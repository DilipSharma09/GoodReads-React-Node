{
  "name": "react-exp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
     "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-scripts": "2.1.8",
    "xml-js": "^1.6.11"
  },
  "scripts": {
   "client-install": "npm install --prefix client",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
     "heroku-postbuild":
      "NPM_CONFIG_PRODUCTION=false npm install --prefix client 
       && npm run build --prefix client"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:8080",
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "request": "^2.88.0"
  }
}
